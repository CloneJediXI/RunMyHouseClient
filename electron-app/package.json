{
  "name": "electron-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@capacitor/android": "3.2.3",
    "@capacitor/cli": "^3.2.3",
    "@capacitor/core": "^3.2.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
